            <kind>1</kind>
            <string>    if(global.pause and !instance_exists(obj_scoreScreen)){
        if(keyboard_check_pressed(vk_up) &amp;&amp; opcao &gt; 0){
            opcao -= 1;
        }
        else if(keyboard_check_pressed(vk_down) &amp;&amp; opcao &lt; 3){
            opcao += 1;
        }
        if(keyboard_check_pressed(ord("X"))){
            switch(opcao){
            case 0:
                    keyboard_key_press(vk_space);
                    break;
            case 1:
                    global.pause = false;
                    global.velPlayer = 1+(2*global.Upgrade[1,0]);
                    room_restart();
                    global.nivelNave = 0;
                    global.km = 0;
                    // Quando X for pressionado, reseta algumas vari√°veis
                    global.combo = 0;
                    global.barraCombo = 100;
                    global.dificuldade = 0
                    global.progresso = 0;
                    global.desafios = false;
                    instance_activate_all();
                    break;
            case 2:
                    room_goto(Canhao);
                    instance_activate_all()
                    global.pause = false;
                    global.upgrade = true;
                    break;
            case 3:
                    game_end();
                    break;
            }
        }
        if(alpha &lt; 1){
            alpha += 0.05;
        }
        y = lerp(y,0,0.1);
    }
